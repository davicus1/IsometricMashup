[application]

config/name="Isometric Mashup"
config/description="This demo shows a traditional isometric view with depth sorting.

A character can move around the level and will also slide around objects,
as well as be occluded when standing in front or behind them."
run/main_scene="res://src/Screens/StartScreen.tscn"
config/icon="res://icon.png"

[autoload]

GameConfig="*res://src/AutoLoad/config.gd"
Music="*res://src/AutoLoad/Music.tscn"
gamestate="*res://src/Network/gamestate.gd"

[display]

window/size/width=1280
window/size/height=900
window/stretch/mode="2d"
window/stretch/aspect="expand"

[gdnative]

singletons=[ "res://git_api.gdnlib" ]

[global]

screen=false
screenshot=false

[image_loader]

filter=false
gen_mipmaps=false

